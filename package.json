{
  "name": "asset-store-price-tracker",
  "version": "2.2.1",
  "description": "This extension lets you view pricing history for asset pricing on both Unity's and Unreal's webstores.",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run build-scripts && npm run build-service_worker",
    "build-scripts": "npm run clean-scripts && webpack --env=mode=production --env=background=scripts --config webpack.config.js && cd out/scripts/ && bestzip %npm_package_name%_v%npm_package_version%_scripts.zip *",
    "build-service_worker": "npm run clean-service_worker && webpack --env=mode=production --env=background=service_worker --config webpack.config.js && cd out/service_worker/ && bestzip %npm_package_name%_v%npm_package_version%_service_worker.zip *",
    "clean": "rimraf out/* --glob",
    "clean-scripts": "rimraf out/scripts/* --glob",
    "clean-service_worker": "rimraf out/service_worker/* --glob",
    "dev": "npm run clean && npm run dev-scripts && npm run dev-service_worker",
    "dev-scripts": "npm run clean-scripts && webpack --env=mode=development --env=background=scripts --config webpack.config.js",
    "dev-service_worker": "npm run clean-service_worker && webpack --env=mode=development --env=background=service_worker --config webpack.config.js"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/chrome": "^0.0.287",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/jquery": "^3.5.32",
    "@types/validator": "^13.12.2",
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "file-loader": "^6.2.0",
    "html-minimizer-webpack-plugin": "^5.0.0",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "imagemin": "^9.0.0",
    "json-minimizer-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sharp": "^0.33.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "tailwindcss": "^3.4.17",
    "terser-webpack-plugin": "^5.3.11",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "webextension-polyfill": "^0.12.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "asset-store-price-tracker": "file:",
    "bestzip": "^2.2.1",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.7",
    "glob": "^11.0.0",
    "jquery": "^3.7.1",
    "rimraf": "^6.0.1",
    "validator": "^13.12.0"
  },
  "browserslist": [
    ">0%"
  ]
}
